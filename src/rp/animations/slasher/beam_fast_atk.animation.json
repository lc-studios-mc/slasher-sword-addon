{
	"format_version": "1.8.0",
	"animations": {
		"animation.slasher_beam_fast_atk.general": {
			"loop": true,
			"bones": {
				"xy": {
					"rotation": ["v.rotation_x", "v.rotation_y", 0],
					"scale": "v.is_visible ? 1 : 0"
				},
				"z": {
					"rotation": [0, 0, "v.rotation_z"]
				},
				"left_bit": {
					"position": [-10, 0, 0]
				},
				"right_bit": {
					"position": [10, 0, 0]
				}
			}
		},
		"animation.slasher_beam_fast_atk.bits": {
			"loop": true,
			"bones": {
				"left_bit": {
					"scale": "v.bit == 0 ? 1 : 0"
				},
				"center_bit": {
					"scale": "v.bit == 1 ? 1 : 0"
				},
				"right_bit": {
					"scale": "v.bit == 2 ? 1 : 0"
				}
			}
		},
		"animation.slasher_beam_fast_atk.trail_left": {
			"loop": true,
			"animation_length": 1,
			"particle_effects": {
				"0.0": {
					"effect": "trail",
					"locator": "trail_left"
				}
			}
		},
		"animation.slasher_beam_fast_atk.trail_right": {
			"loop": true,
			"animation_length": 1,
			"particle_effects": {
				"0.0": {
					"effect": "trail",
					"locator": "trail_right"
				}
			}
		}
	}
}